<script type="text/javascript">
// <![CDATA[
/* ½ºÆä¼È BG */
function setSpecialBg(jsonTxt) {
	try {
		if (jsonTxt) {
			var jsonData = eval("("+jsonTxt+")");
			// left_bg, left_link, right_bg, right_link
			$('left_bg').style.backgroundImage ='url(' + jsonData.left_bg + ')';
			$('right_bg').style.backgroundImage ='url(' + jsonData.right_bg + ')';
		}
	} catch(e) {}
}
function resetSpecialBg() {
	$('left_bg').style.backgroundImage ='';
	$('right_bg').style.backgroundImage ='';
}
/* ¿À´Ãº»¿µÈ­ */
function setTodayMovieCount(today) {
	var url = '/slideCount';
	var pars = 'pos=2&today='+today;

	new Empas.Ajax.Request(url, 
		{method: 'get',
		parameters: pars,
		onComplete: updateTodayMovieCount,
		onFailure: function() {
			window.alert("¿À´Ã º» ¿µÈ­ °¹¼ö¸¦ °¡Á®¿Ã¼ö ¾ø½À´Ï´Ù.");
		}});
}
function updateTodayMovieCount(req) {
	var jsonTxt = req.responseText;
	if (jsonTxt.trim().length > 0) {
		var jsonData = eval("("+jsonTxt+")");
		$('todaymovieCount').innerHTML = jsonData.count;
	}
}
/* ¿¹¸Å¹Ù±¸´Ï */
function setBasketCount() {
	$('basketCount').innerHTML = '0';
	return;
}
function updateBasketCount(req) {
	var jsonTxt = req.responseText;
	if (jsonTxt.trim().length > 0) {
		var jsonData = eval("("+jsonTxt+")");
		$('basketCount').innerHTML = jsonData.count;
	}
}
/* ¿¹¸ÅÈ®ÀÎ/Ãë¼Ò */
function setReserveCount() {
	$('reserveCount').innerHTML = '0';
	return;
}
function updateReserveCount(req) {
	var jsonTxt = req.responseText;
	if (jsonTxt.trim().length > 0) {
		var jsonData = eval("("+jsonTxt+")");
		$('reserveCount').innerHTML = jsonData.count;
	}
}
function reloadTodayMovie(today) {
	setTodayMovieCount(today);
	if (this.currentMenu == 2) {
		var url = '/slide/todaymovie';
		changeSlideFrame('left',url);
	}
}
function init() {
//	resizeWin();
	setTodayMovieCount('');
}
// slide control
var currentMenu = 0;
/* ½½¶óÀÌµå open/close */
function showSlide() {
	if ($('contentBox').style.display == 'none') {
		$('contentBox').style.display = 'block';
		//$('layerControlBtn').src = '/img/btn_slide_close.gif';
		$('slide_open').className = 'contentLayerControl close';
		$('slide_open').blur();
		if (this.currentMenu == 0) {
			setSlideTop10NDR();
		}
	} else {
		//$('layerControlBtn').src = '/img/btn_slide_open.gif';
		$('slide_open').className = 'contentLayerControl open';
		$('slide_open').blur();
		$('contentBox').style.display = 'none';
	}
	/*
	if ((navigator.userAgent.indexOf('MSIE 8.0') == 0) && (navigator.userAgent.indexOf('MSIE 6.0') != -1)) {
		SetSlidePosition();
	}
	*/
}
/* ½½¶óÀÌµå open Ã¼Å© */
function checkSlide() {
	if ($('contentBox').style.display == 'none') {
		$('contentBox').style.display = 'block';
		$('slide_open').className = 'contentLayerControl close';
		$('slide_open').blur();
	} else return;
}
/* ½½¶óÀÌµå ¼±ÅÃ */
function selectSlide(pos) {
	var bLoginCheck = true;
	var bPositionCheck = true;
	if (pos < 3) {
		bPositionCheck = false;
		if (this.currentMenu == pos) {
			showSlide();
			return;
		} else {
			checkSlide();
		}
	}

	if (bPositionCheck && bLoginCheck) {
		CmdLogin('http://movie.nate.com/mag');
	} else {
		var nMenu = 4;
		var className = '';
		var onOff = '';
		for (var i = 1; i <= nMenu; i++) {
			if (i == 1) {
				className = 'tab home';
			} else if (i == nMenu) {
				className = 'tab end';
			} else {
				className = 'tab';
			}
			if (i == pos) {
				className += ' selected';
				$('slide_img_'+i).src='/img/btn_slide_tab0'+i+'_on.gif';
			} else {
				$('slide_img_'+i).src='/img/btn_slide_tab0'+i+'.gif';
			}
			$('slide_tab_'+i).className = className;
		}
		var url = '';
		switch (pos) {
			case '1' :
				url = '/slide/top10';
				setSlideTop10NDR();
				break;
			case '2' :
				url = '/slide/todaymovie';
				break;
			case '3' :
				url = '/slide/reserve_basket';
				break;
			case '4' :
				url = '/slide/reserve_check';
				break;
		}

		changeSlideFrame('left',url);
		if (this.currentMenu == pos) {
			showSlide();
		} else {
			checkSlide();
		}
		this.currentMenu = pos;
	}
}
function setSlideTop10NDR() {
	$('slide_ndr').src = 'http://movie.nate.com/_template/content/slide/top10_ndr.html';
}
function selectSlideNotMove(pos) {
	var nMenu = 4;
	var className = '';
	var onOff = '';
	for (var i = 1; i <= nMenu; i++) {
		if (i == 1) {
			className = 'tab first';
		} else if (i == nMenu) {
			className = 'tab end';
		} else {
			className = 'tab';
		}
		if (i == pos) {
			className += ' selected';
			$('slide_img_'+i).src='/img/btn_slide_tab0'+i+'_on.gif';
		} else {
			$('slide_img_'+i).src='/img/btn_slide_tab0'+i+'.gif';
		}
		$('slide_tab_'+i).className = className;
	}
}
function SetSlidePosition() {
	var PageHeight = document.documentElement.offsetHeight;	// È­¸é ³ôÀÌ
	var ScrollTop = document.documentElement.scrollTop;		// ½ºÅ©·Ñ µÈ À§Ä¡
	var SlideLayerHeight = $('slideWraper').offsetHeight;	// ½½¶óÀÌµå ·¹ÀÌ¾î ³ôÀÌ
	var exception = 0; // ¿¡·¯°ª
	//var ScrollWidth = document.documentElement.offsetHeight - document.documentElement.clientHeight;

	if (PageHeight + ScrollTop > document.body.clientHeight) {
		exception = PageHeight + ScrollTop - document.body.clientHeight
	}
	$('slideWraper').style.top = PageHeight - SlideLayerHeight + ScrollTop - exception;
}
/*
if ((navigator.userAgent.indexOf('MSIE 8.0') == 0) && (navigator.userAgent.indexOf('MSIE 6.0') != -1)) {
	window.onscroll = function() {SetSlidePosition();}
}
*/
try {
	$ONDOMLOAD(init);
} catch (e) {}

// ]]>
</script>

