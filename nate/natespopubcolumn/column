	var sTab = GetCookie('right_tab');
	if ( sTab != '' && sTab.substr(0,1) == 'i' ) {
		try { 
			setTab(sTab);
		} catch(e) {
		}
	}
	setCookie("right_tab",null,null,'/','news.nate.com');

	if ( isiPad() ) {
		jQuery('#tab_int_on a').bind('click',function(event) {
			var index = jQuery(this).prop('id').substr(-1);
			setRankingTab(4,index);
			event.preventDefault();
		});
