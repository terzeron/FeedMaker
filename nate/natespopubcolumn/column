<!-- 알림 토스트// -->
<div id="miniToast" class="miniToast" style="display:none">
        <dl>
                <dt>이 시각 베플 뉴스</dt>
                <dd class="desc"><a href="http://www.nate.com/?f=timeline" onclick="ndrclick('RMW30');" id="bepleNews">이 시각 베플 뉴스</a></dd>
                <dd class="bnMore"><a href="http://www.nate.com/?f=timeline" onclick="ndrclick('RMV79');">베플 뉴스 더보기</a></dd>
                <dd><a href="http://newscomm.nate.com/enews/noticeView?bbs_grp_gb=NOTICE&amp;bbs_sq=299&amp;ctgr_cd=N&amp;post_sq=2770125&amp;page=1" target="_blank" class="btnHelp">도움말</a></dd>
        </dl>
        <div class="chkClose"><input type="checkbox" id="chkClose" onclick="ndrclick('RMV80'); CloseToast(1)" /><label for="chkClose">일주일간 안보기</label></div>
        <a href="javascript:void(0);" onclick="CloseToast()" class="btnClose">닫기</a>
</div>
<script type="text/javascript">
//<![CDATA[
var checkDenyMid = [];
var isFirstCall = 0;
var nStartTime = 6;
var nEndTime = 23;
var stdTimeInterval = 60 * 60;
var toastId;
var bottomMargin = 0;
var toastTimer;
var updateArticleNum = 0;
var isTrace = false;
var stdReverseInterval = 10;
var showSpeed = (Prototype.Browser.IE && navigator.userAgent.indexOf('MSIE 8') > -1) ? 5:((Prototype.Browser.IE && navigator.userAgent.indexOf('MSIE 9') > -1) ? 2.5:1);
var sparameter = '';
tracelog = function (logtxt) { 
    if (true === isTrace) { 
        try { console.log(logtxt); } catch(e) {} 
    } 
}
jsonHandler = function (sobj) { 
try {
    var clientTime = Math.floor((new Date).getTime() / 1000);
    // diff between c and s
    // check server's next reload time
    var nextReloadTime = stdTimeInterval - (sobj.stime % stdTimeInterval);
    tracelog('call back json handler');
    tracelog('client time : ' + clientTime + ', next reload time : ' + nextReloadTime);     

    toastId = setInterval(CheckUpdate, nextReloadTime * 1000);
    isFirstCall++;
} catch(e) {}
finally {
    clientTime = null;
    nextReloadTime = null;
}
}

updateHandler = function (sobj) {
        tracelog('call back update handler');
        tracelog('sobj title : ' + sobj.artc_title + ', sobj cnt : ' + sobj.artc_cnt + ', sobj sq : ' + sobj.artc_sq);
        if (typeof sobj == 'object') updateArticleNum = 1;
        $('bepleNews').innerHTML = sobj.artc_title; 
        //$('alarmNum').innerHTML = parseInt(updateArticleNum);
        //ChangeTitle(sobj.returncount);
        ToggleToast(1);
}

CheckTimeDiff = function () {
    var sdate = new Date();
    var sdate_hour = sdate.getHours();
    var sdate_min = sdate.getMinutes();

    if (parseInt(sdate_hour) < nStartTime) {
          tracelog('stop with std time');
          return;
    }

    sparameter = ((sdate_hour < 10) ? '0' + sdate_hour.toString():sdate_hour.toString()) + ((sdate_min < 10) ? '0' + sdate_min.toString():sdate_min.toString());
    if (isFirstCall > 0) return;
    else {
        // check deny service
        var smid = 's';
        if (smid.length > 0) {
            for (var i = 0, length = checkDenyMid.length; i < length; i++) {
                if (checkDenyMid[i] == smid) {
                    Event.stopObserving(window,'load',CheckTimeDiff,false);
                    return;
                }
            }
        }
    }
    try {
        var svUrl = 'http://news.nate.com/Main/NewsBox/getServerTime?'+sparameter;
        var scriptNode = document.createElement('script');
        scriptNode.type = 'text/javascript';
        scriptNode.src = svUrl;
        var head = document.getElementsByTagName('head')[0];
        head.appendChild(scriptNode);
    } catch(e) { tracelog('CheckTimeDiff message : ' + e.message); }
    finally {
        scriptNode = null;
        head = null;
        svUrl = null;
    }
	sdate = null;
	sdate_hour = null;
	sdate_min = null;
}

Event.observe(window, 'load', CheckTimeDiff);
var bottomMargin;
var bottomMarginReverse = 0;
var toastTimer;
ChangeTitle = function () {
try {
    var iUpdateNum = (arguments.length > 0) ? parseInt(arguments[0]):0; 

    var regExp = /\(\d{1,}\)/g;
    var changeCount = (parseInt(iUpdateNum) > 0) ? '(' + parseInt(iUpdateNum) + ')':'';
    if (regExp.test(document.title)) {
        document.title = (parseInt(iUpdateNum) > 0) ? document.title.replace(document.title.match(regExp), changeCount):document.title.replace(document.title.match(regExp),'');
    } else {
		if (parseInt(iUpdateNum) > 0) {
        	document.title = '(' + parseInt(iUpdateNum) + ')' + document.title;
		}
    }
} catch(e) {}
finally {
    iUpdateNum = null;
    changeCount = null;
}
}

CheckUpdate = function () {
    tracelog('call check update');
    updateArticlNum = 0;
    bottomMargin = 0;

    // check cookie 
    if (GetCookie('news_toast') == 'n') {
        clearInterval(toastId);
        return;
    }

    // check new article by timestamp (auto take by server side)
    try {
	var checkUrl = 'http://news.nate.com/Main/NewsBox/getAlarmFeed?'+sparameter;
        var scriptNode = document.createElement('script');
        scriptNode.type = 'text/javascript';
        scriptNode.src = checkUrl;
        var head = document.getElementsByTagName('head')[0];
        head.appendChild(scriptNode);
    } catch(e) { }
    finally {
        scriptNode = null;
        head  = null;
        checkUrl = null;
    }

    if (isFirstCall >= 1) {
        clearInterval(toastId);
        toastId = setInterval(CheckUpdate, stdTimeInterval * 1000); 
    }
    isFirstCall++;
}

var toastReverseId;
var toastReverseTimer;
var bOverState = 0;
var toastDirectionInfo = 'n';

ToastObserver = function (event) {
try {
    var evt = event || window.event;
    bOverState = (evt['type'] == 'mouseover') ? 1:0;

    if (1 === bOverState) {
        tracelog('mouse over capture');
        $('miniToast').style.bottom = '0px';
        clearInterval(toastReverseId);
    } else {
        tracelog('mouse out capture');
        clearInterval(toastReverseId);
        toastReverseId = setInterval(ToggleToast , stdReverseInterval * 1000);
    }
} catch(e) {}
finally {
    evt = null;
}   
}

$('miniToast').observe('mouseover', ToastObserver).observe('mouseout', ToastObserver); 
ToggleToast = function () {
try {
    if (updateArticleNum == 0) return;
    var bDirection = arguments[0] || 0;

    if (1 === bDirection) { // up
        tracelog('-----call toggle toast with 1');
        if (updateArticleNum == 0 || 1 === bOverState || toastDirectionInfo == 'd') {
            clearTimeout(toastTimer);
            toastTimer = null;
            bottomMargin = 0;
            clearInterval(toastReverseId);
            toastReverseId = null;
            return; 
        }

        toastDirectionInfo = 'u';
        bottomMargin += showSpeed;

        $('miniToast').style.display = 'block';
        $('miniToast').style.bottom = (bottomMargin - 160) + 'px';
        tracelog((new Date).getTime() + 'show toast bottom margin : ' + bottomMargin);
        if (bottomMargin < 160) {
            toastTimer = setTimeout("ToggleToast(1)", 5);
        } else {
            toastDirectionInfo = 'n';
            clearTimeout(toastTimer);
            toastTimer = null;
            bottomMargin = 0;
            clearInterval(toastReverseId);
            toastReverseId = setInterval(ToggleToast, stdReverseInterval * 1000);
        }
    } else { // down
        tracelog('-----call toggle toast with 0');
        tracelog('-----call toggle direction : ' + toastDirectionInfo); 

        if (1 === bOverState || toastDirectionInfo == 'u' || $('miniToast').style.display != 'block') {
            toastDirectionInfo = 'n';
            bottomMarginReverse = 0;
            clearTimeout(toastReverseTimer);
            toastReverseTimer = null;
            clearInterval(toastReverseId);
            toastReverseId = null;
            return;
        }

        toastDirectionInfo = 'd';
        bottomMarginReverse -= showSpeed;

        $('miniToast').style.bottom = bottomMarginReverse + 'px';
        tracelog((new Date).getTime() + 'show toast bottom margin : ' + bottomMarginReverse);
        if (bottomMarginReverse > -159) {
            toastReverseTimer = setTimeout(ToggleToast, 5);
        } else {
            tracelog('release reverse');
            toastDirectionInfo = 'n';
            bottomMarginReverse = 0;
            clearTimeout(toastReverseTimer);
            toastReverseTimer = null;
            clearInterval(toastReverseId);
            toastReverseId = null;
        }
    }
} catch(e) {}
finally {
    bDirection = null;  
}
}

CloseToast = function () {
try {
    var bCheckArg = arguments[0] || 0;
    if (1 === bCheckArg) {
        tracelog('set cookie');
        // make cookie
        // expire to 23:59:59
        var expireTimeLeft = 0;
        var nowDate = new Date;
        tracelog('expires : ' + nowDate.getFullYear() + ' ' + (nowDate.getMonth()+1) + ' ' + (nowDate.getDate()) + ' 23:59:59:000');
        var expireDate =  new Date(nowDate.getFullYear(), nowDate.getMonth(), nowDate.getDate()+6, 23,59,59,000);
        setCookie('news_toast', 'n', expireDate, '/', getDomain(), false);  
        ChangeTitle();
        expireTimeLeft = null;
        nowDate = null;
        expireDate = null;
    } else {
        ChangeTitle(updateArticleNum);
    }

    clearInterval(toastId);
    $('miniToast').style.display = 'none';
} catch(e) {}
finally {
    bCheckArg = null;
}
}
//]]>
</script>
<!-- //알림 토스트 -->
</body></html>