<script type="text/javascript">
//<![CDATA[
function iPadNonDisplaySpaceBanner(){
	var uAgent = navigator.userAgent.toLowerCase();
	var mobilePhones = new Array('ipad');

	for(var i=0;i<mobilePhones.length;i++){
		if(uAgent.indexOf(mobilePhones[i]) != -1){
			document.getElementById('ad_newsMarginCube_right').style.display='none';
		}
	}
}

if (window.addEventListener) {
	window.addEventListener("load", iPadNonDisplaySpaceBanner,false); 
} else if (window.attachEvent) {
	window.attachEvent("onload", iPadNonDisplaySpaceBanner); 
}
//]]>
</script>			<div id="cntArea">
