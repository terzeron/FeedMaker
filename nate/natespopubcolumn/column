	function fnControlAd() {
		try {
			var leftAd = document.getElementById('ad_newsMarginCube_left');
			if(leftAd) {
				leftAd.innerHTML = '<iframe src="http://sports.news.nate.com/etc/adSpace?mode=left&mid=s1001" width="110" height="223" title="날개배너 광고"  scrolling="no" frameborder="0" marginheight="0" marginwidth="0"></iframe>';
			}
			
			var rightAd = document.getElementById('ad_newsMarginCube_right');
			if(rightAd) {
				rightAd.innerHTML = '<iframe src="http://sports.news.nate.com/etc/adSpace?mode=right&mid=s1001" width="110" height="223" title="날개배너 광고"  scrolling="no" frameborder="0" marginheight="0" marginwidth="0"></iframe>';
			}
		} catch(e) {
		}
	}

	if (window.addEventListener) {
		window.addEventListener("load", fnControlAd,false);  
	} else if (window.attachEvent) { 
		window.attachEvent("onload", fnControlAd);  
	}
	//]]>
