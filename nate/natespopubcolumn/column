<script type="text/javascript">
//<![CDATA[
function setTab(sType,num) {
	var tab = new Array('foot','base','gen');
	var ulTab = document.getElementById('tab_ul');
	for (i=0;i<tab.length;i++) {
		var elemDiv = document.getElementById(tab[i]+"_list");
		if ( sType == tab[i] ) {
			elemDiv.style.display = '';
			} else {
			elemDiv.style.display = 'none';
		}
	}
	ulTab.className = 'pubColumnNistTab dateMenu'+num;
	setCookie("column_tab",sType,null,'/');
}

window.onload = function () {
	var tab = "foot";
	if ( tab == 'foot' ) {
		setTab(tab,1);
	} else if ( tab == 'base' ) {
		setTab(tab,2);
	} else if ( tab == 'gen' ) {
		setTab(tab,3);
	} else {
		setTab('foot',1);
	}
}
//]]>
</script> 
