        var search_form_init_v = false;
        function search_form_init() {
			bRoll_key = false;
			var elemRoll = document.getElementById('rankDiv');
			var elemInput = document.getElementById('input_search');
			if ( elemRoll ) {
				elemRoll.style.display = 'none';
			}
            if ( !search_form_init_v ) {
				elemInput.value='';
            }
   			elemInput.focus();
			search_form_init_v = true;
            return false;
        }
		function setKeyword() {
			if ( bRoll_key ) {
				bUseKey = true;
				search_form_init_v = true;
				var elemRoll = document.getElementById('rankDiv');
				if ( elemRoll ) {
					elemRoll.style.display = 'none';
				}
				var elemKeyword = document.getElementById('issue_keyword');
				var searchFrm = document.search;
				searchFrm.q.value = elemKeyword.innerHTML;
			}
		}
    //]]>
    </script>
